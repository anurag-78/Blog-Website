
<%- include('partials/header'); -%>
<h1>Home</h1>
<p><%=homeText%></p>
<%posts.forEach(function(post){%>
  <h1><%=post.title;%></h1>
  <p>
    <%=post.content.substring(0,100)+" ..."%><a href="/posts/<%=post._id%>">Read More</a>
    </p>
<%})%>
<div class="buttondiv">
  <button type="button" onclick="location.href='/compose'" class="btn btn-primary">Add Your Own Blog</button>
</div>
<%- include('partials/footer'); -%>
